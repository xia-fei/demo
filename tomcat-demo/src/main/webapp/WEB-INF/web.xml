<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>KeepConnectionServlet</servlet-name>
        <servlet-class>servlet.KeepConnectionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrashTestServlet</servlet-name>
        <servlet-class>servlet.CrashTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrashTestServlet2</servlet-name>
        <servlet-class>servlet.CrashTestServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>servlet.DownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>servlet.CookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KeepConnectionServlet</servlet-name>
        <url-pattern>/keepConnection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrashTestServlet</servlet-name>
        <url-pattern>/crash</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrashTestServlet2</servlet-name>
        <url-pattern>/crash2</url-pattern>
    </servlet-mapping>
</web-app>
